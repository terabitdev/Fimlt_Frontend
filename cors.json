[
  {
    "origin": ["*"],
    "method": ["GET", "HEAD", "OPTIONS"],
    "responseHeader": [
      "Content-Type", 
      "Access-Control-Allow-Origin",
      "Cross-Origin-Embedder-Policy",
      "Cross-Origin-Opener-Policy",
      "Cross-Origin-Resource-Policy",
      "Content-Length",
      "Content-Range"
    ],
    "maxAgeSeconds": 3600
  }
]